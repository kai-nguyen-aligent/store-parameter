{
  "name": "store-parameters",
  "description": "CLI tool for import and export SSM parameters",
  "version": "0.0.7",
  "author": "Kai Nguyen",
  "bin": {
    "store-parameters": "./bin/run.js"
  },
  "bugs": "https://github.com/kai-nguyen-aligent/store-parameters/issues",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.650.0",
    "@aws-sdk/credential-providers": "^3.650.0",
    "@inquirer/prompts": "^5.5.0",
    "@json2csv/node": "^7.0.6",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "csvtojson": "^2.0.10"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kai-nguyen-aligent/store-parameters",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "store-parameters",
    "dirname": "store-parameters",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kai-nguyen-aligent/store-parameters",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
}
